{
  "name": "@flow/core",
  "version": "0.0.1",
  "description": "A functional library for TypeScript/JavaScript",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "pnpm tsc",
    "start": "pnpm esrun ./src/index.ts",
    "dev": "pnpm esrun --watch ./src/*.ts",
    "package": "pnpm rollup -c rollup.config.mjs",
    "debug": "pnpm build && node --inspect-brk ./dist/src/index.js",
    "test": "pnpm jest",
    "test:coverage": "pnpm jest --coverage",
    "lint": "pnpm eslint -f unix --ext .ts .",
    "docs:dev": "pnpm vitepress dev docs",
    "docs:build": "pnpm typedoc && pnpm vitepress build docs",
    "docs:preview": "pnpm vitepress preview docs"
  },
  "keywords": [],
  "author": "niuiic",
  "license": "MIT",
  "devDependencies": {
    "@digitak/esrun": "^3.2.24",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rollup": "^3.23.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.2",
    "typedoc": "0.24.7",
    "typedoc-plugin-markdown": "4.0.0-next.13",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typedoc-plugin-replace-text": "^2.2.0",
    "typedoc-vitepress-theme": "1.0.0-next.2",
    "typescript": "^5.0.4",
    "vitepress": "1.0.0-beta.1"
  }
}
