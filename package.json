{
  "name": "@niuiic/flow",
  "version": "0.1.2",
  "description": "A functional library for TypeScript/JavaScript",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "pnpm tsc",
    "package": "pnpm rollup -c rollup.config.mjs",
    "release": "np",
    "call": "pnpm esrun cli",
    "test": "pnpm jest",
    "test:coverage": "pnpm jest --coverage",
    "lint": "pnpm call lint",
    "docs:dev": "pnpm vitepress dev docs",
    "docs:build": "pnpm typedoc && pnpm call cleanDoc && pnpm vitepress build docs",
    "docs:preview": "pnpm vitepress preview docs"
  },
  "author": "niuiic",
  "homepage": "https://flow.niuiic.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/niuiic/flow.git"
  },
  "keywords": [
    "fp",
    "lazy",
    "concurrency"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@digitak/esrun": "^3.2.24",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "np": "^8.0.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rollup": "^3.25.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.3",
    "typedoc": "0.24.8",
    "typedoc-plugin-markdown": "4.0.0-next.16",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typedoc-plugin-replace-text": "^3.0.0",
    "typedoc-vitepress-theme": "1.0.0-next.2",
    "typescript": "^5.1.3",
    "vitepress": "1.0.0-beta.3"
  }
}
